<%- include('partials/header'); -%>

<h1>Home</h1>
<p> <%= startingContent %> </p>

<% userPosts.forEach(function(post) { %>

  <h2> <%= post.postTitle %> </h2>
  <p>
    <%= post.postBody.substring(0, 100) + ' ...' %>
    <a href="/posts/<%= post.postTitle %>">Read More</a>
  </p>
    
<% }) %>

<%- include('partials/footer'); -%>